<%= form_with(model: link, local: true, url: '/management/link') do | form | %>
  <div class="field">
    <%= form.label :url, 'URL' %>
    <%= form.text_field :url %>
    <br>
    <%= form.label :title, 'サイト名' %>
    <%= form.url_field :title %>
    <br>
    <%= form.label :school, '流派' %>
    <%= form.text_field :school %>
    <br>
    <%= form.label :link_type, 'カテゴリ' %>
    <%= form.text_field :link_type %>
    <br>
    <%= form.submit %>
  </div>
<% end %>
